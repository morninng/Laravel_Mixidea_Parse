<script type="text/template" data-template="discussion_multiple_template">
  <div id="discussion_note_container">
    <div role="tabpanel">
      <ul class="nav nav-tabs" role="tablist">
        <% _.each(list, function(e,i){ %>
          <li class="<%= e.active_str %> tab_name_font"><a href="#<%= e.name %>" aria-controls="<%= e.name %>" data-toggle="tab"><%= e.name %></a></li>
        <% });%>
      </ul>
      <div class="tab-content">
        <% _.each(list, function(e,i){ %>
        <div role="tabpanel" class="tab-pane <%= e.active_str %>" id="<%= e.name %>">
          <div id="argument_pain<%= e.name %>">
          </div>
        </div>
        <% });%>
      </div>
    </div>
  </div>
</script>
  


<script type="text/template" data-template="chat_template">

  <div id="chat_field">
    <div class="msg_box">
      <div class="msg_head">
        <div class="window_title" data-bind="text:chat_header_title"></div>
        <div class="window_icon" data-bind="visible:visible_collapse_button,event:{click:click_collapse}">
          <img src="https://s3.amazonaws.com/mixidea/minimize.png">
        </div>
        <div class="window_icon" data-bind="visible:visible_maximize_button,event:{click:click_expand}">
          <img src="https://s3.amazonaws.com/mixidea/maximize.png">
        </div>
      </div>
      <div id="msg_wrap">
        <div class="msg_body">
          <ul data-bind="foreach: chat_message_array">
            <li>
                  <div data-bind="css:chat_box_class">
                    <span class="chat_sender" data-bind="text:sender_name"></span>
                    <span class="chat_message" data-bind="text:chat_message"></span>
                  </div>
                </li>
          </ul>
        </div>
        <div class="msg_footer">
          <textarea data-bind="textInput:chat_text_input, event:{keyup: onEnterTextbox}" class="msg_input" rows="2">
          </textarea>
        </div>
      </div>
    </div>
  </div>
</script>


<script type="text/template" data-template="impression_template">

  <div id="impression_container">
    <div class="hearhear">
      <input type="image" id="hearhear_img" src="https://s3.amazonaws.com/mixidea/good.png" data-bind="event:{click:click_hearhear}">
      <ul class="impression_ul" data-bind="foreach:{data:hearhear_users, beforeRemove:remove_fadeout } ">
        <li><img class="impression_member_img" data-bind="attr: {src:pict_src}"></li>
      </ul>
    </div>
    <div class="booboo">
      <input type="image" id="booboo_img" src="https://s3.amazonaws.com/mixidea/bad.png" data-bind="click:click_booboo">
      <ul class="impression_ul" data-bind="foreach:{data:booboo_users, beforeRemove:remove_fadeout } ">
        <li><img class="impression_member_img" data-bind="attr: {src:pict_src}"></li>
      </ul>
    </div>
  </div>

</script>



<script type="text/template" data-template="transcription_template">
  <div id="transcription_field">
    <div id="transcript_box" data-bind="visible:visible_transctiption_box" >
      <div class="transcript_head">
        <span data-bind="text:transcript_header_title"></span>

        <div class="window_icon_transcript">
          <div data-bind="event:{click:click_close}">
            <img src="https://s3.amazonaws.com/mixidea/Close.png">
          </div>
        </div>
        <div class="window_icon_transcript" data-bind="visible:visible_collapse_button">
          <div data-bind="event:{click:click_collapse}">
            <img src="https://s3.amazonaws.com/mixidea/minimize.png">
          </div>
        </div>
        <div class="window_icon_transcript" data-bind="visible:visible_maximize_button">
          <div data-bind="event:{click:click_expand}">
            <img src="https://s3.amazonaws.com/mixidea/maximize.png">
          </div>
        </div>
      </div>
      <div id="transcription_wrap">
        <div class="transcript_body" data-bind="event: { mouseover: mouseover_trans_field, mouseout: mouseout_trans_field }">
          <ul data-bind="foreach: transcript_message_array">
            <li>
                  <div data-bind="css:transcription_box_class">
                    <span class="transcription_role" data-bind="text:speaker_role"></span>
                    <span class="transcription_name" data-bind="text:speaker_name"></span>
                    <span class="transcription_message" data-bind="text:transcription_message"></span>
                  </div>
             </li>
          </ul>
        </div>
      </div>
    </div>
  </div>
</script>



<script type="text/html" id="user_status_template">
