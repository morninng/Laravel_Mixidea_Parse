<script type="text/template" data-template="discussion_multiple_template">
  <div id="discussion_note_container">
    <ul class="nav_tabs_custom">
      <% _.each(list, function(e,i){ %>
        <li id="<%= e.name %>_tab" class="<%= e.active_str %>">
